d= input("Enter the date: ")
x=d.split("/")
b=list(map(int,x))
year=(b[2])

if(year%4 == 0):
    if(year%100 == 0):
        if(year%400 == 0):
            print(year,"is Leap Year")
        else:
            print(year,"is not the Leap Year")
    else:
        print(year,"is the Leap Year")
else:
    print(year,"is not the Leap Year")
